<!doctype html>
<!--[if lt IE 7]><html class="no-js ie6 oldie" lang="{{ app.request.locale }}"><![endif]-->
<!--[if IE 7]><html class="no-js ie7 oldie" lang="{{ app.request.locale }}"><![endif]-->
